          <target state="needs-review-translation" state-qualifier="mt-suggestion">・ サウンド ・ オフ</target>
        </trans-unit>
        <trans-unit id="Resx/SoundOn" translate="yes" xml:space="preserve">
          <source>Sound On</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">音します。</target>
        </trans-unit>
        <trans-unit id="Resx/TrialExpired" translate="yes" xml:space="preserve">
          <source>Trial Has Expired.</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">試用版の有効期限が切れています。</target>
        </trans-unit>
        <trans-unit id="Resx/Test" translate="yes" xml:space="preserve">
          <source>test</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">テスト</target>
        </trans-unit>
        <trans-unit id="Resx/TakingPictureText" translate="yes" xml:space="preserve">
          <source>Taking Picture</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">写真を撮る</target>
        </trans-unit>
      </group>
    </body>
  </file>
