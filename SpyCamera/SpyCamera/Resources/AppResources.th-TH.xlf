        </trans-unit>
        <trans-unit id="Resx/NoScreenShotSelected" translate="yes" xml:space="preserve">
          <source>No Screen Shot Selected</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">ไม่มีหน้าจอเลือก</target>
        </trans-unit>
        <trans-unit id="Resx/NotNow" translate="yes" xml:space="preserve">
          <source>Not Now</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">อย่าเพิ่ง</target>
        </trans-unit>
        <trans-unit id="Resx/PictureCounterOff" translate="yes" xml:space="preserve">
          <source>Picture Counter Off</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">นับภาพปิด</target>
        </trans-unit>
        <trans-unit id="Resx/PictureCounterOn" translate="yes" xml:space="preserve">
          <source>Picture Counter On</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">เคาน์เตอร์รูปภาพบน</target>
        </trans-unit>
        <trans-unit id="Resx/PurchaseButton" translate="yes" xml:space="preserve">
          <source>Purchase</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">ซื้อ</target>
        </trans-unit>
        <trans-unit id="Resx/RateButton" translate="yes" xml:space="preserve">
          <source>Rate</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">อัตรา</target>
        </trans-unit>
        <trans-unit id="Resx/SoundOff" translate="yes" xml:space="preserve">
          <source>Sound Off</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">เสียงปิด</target>
        </trans-unit>
        <trans-unit id="Resx/SoundOn" translate="yes" xml:space="preserve">
          <source>Sound On</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">เสียงใน</target>
        </trans-unit>
        <trans-unit id="Resx/TrialExpired" translate="yes" xml:space="preserve">
          <source>Trial Has Expired.</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">ทดลองใช้หมดอายุ</target>
        </trans-unit>
        <trans-unit id="Resx/Test" translate="yes" xml:space="preserve">
          <source>test</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">ทดสอบ</target>
        </trans-unit>
        <trans-unit id="Resx/TakingPictureText" translate="yes" xml:space="preserve">
          <source>Taking Picture</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">การถ่ายภาพ</target>
        </trans-unit>
      </group>
    </body>
