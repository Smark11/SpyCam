        <trans-unit id="Resx/SoundOn" translate="yes" xml:space="preserve">
          <source>Sound On</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">關於聲音</target>
        </trans-unit>
        <trans-unit id="Resx/TrialExpired" translate="yes" xml:space="preserve">
          <source>Trial Has Expired.</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">試用版已過期。</target>
        </trans-unit>
        <trans-unit id="Resx/Test" translate="yes" xml:space="preserve">
          <source>test</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">測試</target>
        </trans-unit>
        <trans-unit id="Resx/TakingPictureText" translate="yes" xml:space="preserve">
          <source>Taking Picture</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">拍照</target>
        </trans-unit>
      </group>
    </body>
