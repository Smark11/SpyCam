          <source>Picture Counter On</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">Картина брояч на</target>
        </trans-unit>
        <trans-unit id="Resx/PurchaseButton" translate="yes" xml:space="preserve">
