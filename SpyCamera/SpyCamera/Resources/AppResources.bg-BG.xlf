          <target state="needs-review-translation" state-qualifier="mt-suggestion">Фокус разстояние</target>
        </trans-unit>
        <trans-unit id="Resx/FocusOn" translate="yes" xml:space="preserve">
          <source>Focus On</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">Съсредоточете се върху</target>
        </trans-unit>
        <trans-unit id="Resx/FrontCamera" translate="yes" xml:space="preserve">
          <source>Front Camera</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">Предна камера</target>
        </trans-unit>
        <trans-unit id="Resx/ImagePreview" translate="yes" xml:space="preserve">
          <source>Image Preview:</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">Визуализация на изображението:</target>
        </trans-unit>
        <trans-unit id="Resx/Instructions" translate="yes" xml:space="preserve">
          <source>Instructions</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">Инструкции</target>
        </trans-unit>
        <trans-unit id="Resx/MainScreen" translate="yes" xml:space="preserve">
          <source>Main Screen</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">Основен екран</target>
        </trans-unit>
        <trans-unit id="Resx/NoScreenShotSelected" translate="yes" xml:space="preserve">
          <source>No Screen Shot Selected</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">Не параван сачми отбран</target>
