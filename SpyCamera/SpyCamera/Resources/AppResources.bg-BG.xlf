        </trans-unit>
        <trans-unit id="Resx/SoundOff" translate="yes" xml:space="preserve">
          <source>Sound Off</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">Звук на разстояние</target>
        </trans-unit>
        <trans-unit id="Resx/SoundOn" translate="yes" xml:space="preserve">
          <source>Sound On</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">Звук на</target>
        </trans-unit>
        <trans-unit id="Resx/TrialExpired" translate="yes" xml:space="preserve">
          <source>Trial Has Expired.</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">Пробна версия е изтекъл.</target>
        </trans-unit>
        <trans-unit id="Resx/Test" translate="yes" xml:space="preserve">
          <source>test</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">тест</target>
        </trans-unit>
        <trans-unit id="Resx/TakingPictureText" translate="yes" xml:space="preserve">
          <source>Taking Picture</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">Правене на снимка</target>
        </trans-unit>
      </group>
    </body>
  </file>
