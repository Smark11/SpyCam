          <source>Sound On</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">关于声音</target>
        </trans-unit>
        <trans-unit id="Resx/TrialExpired" translate="yes" xml:space="preserve">
          <source>Trial Has Expired.</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">试用版已过期。</target>
        </trans-unit>
        <trans-unit id="Resx/Test" translate="yes" xml:space="preserve">
          <source>test</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">测试</target>
        </trans-unit>
        <trans-unit id="Resx/TakingPictureText" translate="yes" xml:space="preserve">
          <source>Taking Picture</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">拍照</target>
        </trans-unit>
      </group>
    </body>
