          <target state="needs-review-translation" state-qualifier="mt-suggestion">निर्देश</target>
        </trans-unit>
        <trans-unit id="Resx/MainScreen" translate="yes" xml:space="preserve">
          <source>Main Screen</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">मुख्य स्क्रीन</target>
        </trans-unit>
        <trans-unit id="Resx/NoScreenShotSelected" translate="yes" xml:space="preserve">
          <source>No Screen Shot Selected</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">कोई स्क्रीन शॉट का चयन</target>
        </trans-unit>
        <trans-unit id="Resx/NotNow" translate="yes" xml:space="preserve">
          <source>Not Now</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">अभी नहीं</target>
        </trans-unit>
        <trans-unit id="Resx/PictureCounterOff" translate="yes" xml:space="preserve">
          <source>Picture Counter Off</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">चित्र काउंटर बंद</target>
        </trans-unit>
        <trans-unit id="Resx/PictureCounterOn" translate="yes" xml:space="preserve">
          <source>Picture Counter On</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">चित्र काउंटर पर</target>
        </trans-unit>
        <trans-unit id="Resx/PurchaseButton" translate="yes" xml:space="preserve">
          <source>Purchase</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">खरीद</target>
        </trans-unit>
        <trans-unit id="Resx/RateButton" translate="yes" xml:space="preserve">
          <source>Rate</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">दर</target>
        </trans-unit>
        <trans-unit id="Resx/SoundOff" translate="yes" xml:space="preserve">
          <source>Sound Off</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">बंद ध्वनि</target>
        </trans-unit>
        <trans-unit id="Resx/SoundOn" translate="yes" xml:space="preserve">
          <source>Sound On</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">पर ध्वनि</target>
        </trans-unit>
        <trans-unit id="Resx/TrialExpired" translate="yes" xml:space="preserve">
          <source>Trial Has Expired.</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">परीक्षण समाप्त हो गया है।</target>
        </trans-unit>
        <trans-unit id="Resx/Test" translate="yes" xml:space="preserve">
          <source>test</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">परीक्षण</target>
        </trans-unit>
        <trans-unit id="Resx/TakingPictureText" translate="yes" xml:space="preserve">
          <source>Taking Picture</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">चित्र लेना</target>
        </trans-unit>
      </group>
    </body>
